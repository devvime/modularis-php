{
    "name": "devvime/modularis",
    "description": "A Minimal and Expressive PHP Micro Routing Framework",
    "keywords": [
        "php",
        "framework",
        "routes"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Modularis\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "devvime"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "require": {
        "PHP": "^8.2.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
